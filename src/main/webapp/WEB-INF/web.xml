<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
          http://java.sun.com/xml/ns/javaee/web-app_2_3.xsd"
         version="2.3">
    <display-name>HotelEase</display-name>
    <servlet>
        <servlet-name>AppointmentServlet</servlet-name>
        <servlet-class>com.example.devops.controller.AppointmentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AppointmentServlet</servlet-name>
        <url-pattern>/bookAppointment</url-pattern>
    </servlet-mapping>
</web-app>
